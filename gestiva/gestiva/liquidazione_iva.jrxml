	<queryString>
		<![CDATA[SELECT uo.CD_UNITA_ORGANIZZATIVA, uo.DS_UNITA_ORGANIZZATIVA, a.PARTITA_IVA as PARTITA_IVA_ENTE, a.RAGIONE_SOCIALE as RAGIONE_SOCIALE_ENTE
FROM UNITA_ORGANIZZATIVA uo,
configurazione_cnr c ,
ANAGRAFICO a
WHERE uo.CD_UNITA_ORGANIZZATIVA =  $P{cd_unita_organizzativa} 
and c.cd_chiave_secondaria(+)='CODICE_ANAG_ENTE'
and c.cd_chiave_primaria(+)='COSTANTI'
AND a.CD_ANAG(+) = c.im01]]>
