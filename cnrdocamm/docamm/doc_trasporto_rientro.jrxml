	<queryString>
		<![CDATA[SELECT DISTINCT
    TESTATA.PG_INVENTARIO,
    TESTATA.TI_DOCUMENTO,
    TESTATA.ESERCIZIO,
    TESTATA.PG_DOC_TRASPORTO_RIENTRO,
    TESTATA.DS_DOC_TRASPORTO_RIENTRO,
    TESTATA.DATA_REGISTRAZIONE,
    TESTATA.CD_TIPO_TRASPORTO_RIENTRO,
    TESTATA.STATO,
    TESTATA.DESTINAZIONE,
    TESTATA.INDIRIZZO,
    TESTATA.FL_INCARICATO,
    TESTATA.FL_VETTORE,
    TESTATA.CD_TERZO_ASSEGNATARIO,
    TESTATA.NOTE_RITIRO,
    TESTATA.NOTE,
    TESTATA.TI_DOCUMENTO_RIF,
    TESTATA.ESERCIZIO_RIF,
    TESTATA.PG_DOC_TRASPORTO_RIENTRO_RIF,
    DETTAGLIO.NR_INVENTARIO,
    DETTAGLIO.PROGRESSIVO,
    DETTAGLIO.INTERVALLO,
    DETTAGLIO.QUANTITA,
    DETTAGLIO.DATA_EFFETTIVA_MOVIMENTAZIONE,
    DETTAGLIO.TI_DOCUMENTO_RIF AS DETT_TI_DOCUMENTO_RIF,
    DETTAGLIO.ESERCIZIO_RIF AS DETT_ESERCIZIO_RIF,
    DETTAGLIO.PG_DOC_TRASPORTO_RIENTRO_RIF AS DETT_PG_DOC_TRASPORTO_RIENTRO_RIF,
    INVENTARIO.DS_BENE,
    INVENTARIO.CD_UNITA_ORGANIZZATIVA,
    INVENTARIO.CD_ASSEGNATARIO,
    UNITA_ORGANIZZATIVA.DS_UNITA_ORGANIZZATIVA,
    TIPO_TRASPORTO_RIENTRO.DS_TIPO_TRASPORTO_RIENTRO,
    TIPO_TRASPORTO_RIENTRO.FL_ABILITA_NOTE,
    (SELECT CD_TERZO||' - '||DENOMINAZIONE_SEDE 
       FROM TERZO
       WHERE CD_TERZO = INVENTARIO_CONSEGNATARIO.CD_CONSEGNATARIO) CONSEGNATARIO,
    (SELECT DENOMINAZIONE_SEDE 
       FROM TERZO
       WHERE CD_TERZO = INVENTARIO_CONSEGNATARIO.CD_DELEGATO) DELEGATO,
    (SELECT CD_CATEGORIA_GRUPPO||' '||DS_CATEGORIA_GRUPPO 
       FROM CATEGORIA_GRUPPO_INVENT
       WHERE CD_CATEGORIA_GRUPPO = INVENTARIO.CD_CATEGORIA_GRUPPO) CATEGORIA_GRUPPO,
    INVENTARIO.TI_COMMERCIALE_ISTITUZIONALE,
    INVENTARIO.ETICHETTA,
    (SELECT DENOMINAZIONE_SEDE 
       FROM TERZO
       WHERE CD_TERZO = TESTATA.CD_TERZO_ASSEGNATARIO) NOME_INCARICATO,
    CASE
        WHEN TESTATA.FL_VETTORE = 'Y' THEN
            SUBSTR(
                (SELECT UO.DS_UNITA_ORGANIZZATIVA 
                 FROM TERZO T
                 JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                 JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                 WHERE T.CD_TERZO = INVENTARIO.CD_ASSEGNATARIO),
                1,
                CASE 
                    WHEN INSTR((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                FROM TERZO T
                                JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                WHERE T.CD_TERZO = INVENTARIO.CD_ASSEGNATARIO), '-') > 0
                    THEN INSTR((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                FROM TERZO T
                                JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                WHERE T.CD_TERZO = INVENTARIO.CD_ASSEGNATARIO), '-') - 1
                    ELSE LENGTH((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                 FROM TERZO T
                                 JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                 JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                 WHERE T.CD_TERZO = INVENTARIO.CD_ASSEGNATARIO))
                END
            )
        WHEN TESTATA.FL_INCARICATO = 'Y' THEN
            SUBSTR(
                (SELECT UO.DS_UNITA_ORGANIZZATIVA 
                 FROM TERZO T
                 JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                 JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                 WHERE T.CD_TERZO = TESTATA.CD_TERZO_ASSEGNATARIO),
                1,
                CASE 
                    WHEN INSTR((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                FROM TERZO T
                                JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                WHERE T.CD_TERZO = TESTATA.CD_TERZO_ASSEGNATARIO), '-') > 0
                    THEN INSTR((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                FROM TERZO T
                                JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                WHERE T.CD_TERZO = TESTATA.CD_TERZO_ASSEGNATARIO), '-') - 1
                    ELSE LENGTH((SELECT UO.DS_UNITA_ORGANIZZATIVA 
                                 FROM TERZO T
                                 JOIN ANAGRAFICO A ON T.CD_ANAG = A.CD_ANAG
                                 JOIN UNITA_ORGANIZZATIVA UO ON A.CD_UNITA_ORGANIZZATIVA = UO.CD_UNITA_ORGANIZZATIVA
                                 WHERE T.CD_TERZO = TESTATA.CD_TERZO_ASSEGNATARIO))
                END
            )
    END AS DS_UO_INC_VETT,
    (SELECT LISTAGG(TTR.DS_TIPO_TRASPORTO_RIENTRO, '|') WITHIN GROUP (ORDER BY TTR.DS_TIPO_TRASPORTO_RIENTRO)
     FROM TIPO_TRASPORTO_RIENTRO TTR
     WHERE TTR.DT_CANCELLAZIONE IS NULL
       AND TTR.TI_DOCUMENTO = TESTATA.TI_DOCUMENTO) LISTA_TIPOLOGIE
FROM DOC_TRASPORTO_RIENTRO TESTATA
JOIN DOC_TRASPORTO_RIENTRO_DETT DETTAGLIO
    ON DETTAGLIO.PG_INVENTARIO = TESTATA.PG_INVENTARIO
   AND DETTAGLIO.TI_DOCUMENTO = TESTATA.TI_DOCUMENTO
   AND DETTAGLIO.ESERCIZIO = TESTATA.ESERCIZIO
   AND DETTAGLIO.PG_DOC_TRASPORTO_RIENTRO = TESTATA.PG_DOC_TRASPORTO_RIENTRO
JOIN INVENTARIO_BENI INVENTARIO
    ON DETTAGLIO.NR_INVENTARIO = INVENTARIO.NR_INVENTARIO
   AND DETTAGLIO.PG_INVENTARIO = INVENTARIO.PG_INVENTARIO
   AND DETTAGLIO.PROGRESSIVO = INVENTARIO.PROGRESSIVO
JOIN UNITA_ORGANIZZATIVA
    ON INVENTARIO.CD_UNITA_ORGANIZZATIVA = UNITA_ORGANIZZATIVA.CD_UNITA_ORGANIZZATIVA
JOIN TIPO_TRASPORTO_RIENTRO
    ON TESTATA.CD_TIPO_TRASPORTO_RIENTRO = TIPO_TRASPORTO_RIENTRO.CD_TIPO_TRASPORTO_RIENTRO
JOIN INVENTARIO_CONSEGNATARIO
    ON TESTATA.PG_INVENTARIO = INVENTARIO_CONSEGNATARIO.PG_INVENTARIO
   AND INVENTARIO_CONSEGNATARIO.DT_INIZIO_VALIDITA < SYSDATE
   AND (INVENTARIO_CONSEGNATARIO.DT_FINE_VALIDITA > SYSDATE OR INVENTARIO_CONSEGNATARIO.DT_FINE_VALIDITA IS NULL)
WHERE TESTATA.ESERCIZIO = $P{esercizio}
  AND TESTATA.PG_INVENTARIO = $P{pg_inventario}
  AND TESTATA.TI_DOCUMENTO = $P{ti_documento}
  AND TESTATA.PG_DOC_TRASPORTO_RIENTRO = $P{pg_doc_trasporto_rientro}]]>
	</queryString>
